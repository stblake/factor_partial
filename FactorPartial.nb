       RowBox[{"OptionsPattern", "[", "]"}]}]}], "]"}], "/;", 
    RowBox[{
     RowBox[{"PolynomialQ", "[", 
      RowBox[{"poly", ",", " ", 
       RowBox[{"Variables", "[", "poly", "]"}]}], "]"}], " ", "&&", " ", 
     RowBox[{
      RowBox[{"Length", "[", 
       RowBox[{"Variables", "[", "poly", "]"}], "]"}], " ", "==", " ", 
      "1"}]}]}], " ", ":=", " ", 
   RowBox[{"partialFactor", "[", 
    RowBox[{"poly", ",", " ", 
     RowBox[{
      RowBox[{"Variables", "[", "poly", "]"}], "[", 
      RowBox[{"[", "1", "]"}], "]"}], ",", " ", "opts"}], "]"}]}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"partialFactor", "[", 
   RowBox[{"poly_", ",", "x_", ",", " ", 
    RowBox[{"OptionsPattern", "[", "]"}]}], "]"}], ":=", 
  RowBox[{"Module", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "maxtime", ",", "deg", ",", "maxdeg", ",", "lc", ",", "divs", ",", 
      "excess", ",", " ", "factored", ",", "\[IndentingNewLine]", "cl", ",", 
      "expandedForm", ",", "eqns", ",", "unks", ",", "soln", ",", "C", ",", 
      "V"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"maxtime", "=", 
      RowBox[{"OptionValue", "[", "\"\<SingleStepTimeConstraint\>\"", "]"}]}],
      ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"deg", "=", 
      RowBox[{"Exponent", "[", 
       RowBox[{"poly", ",", "x"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"maxdeg", "=", 
      RowBox[{"Ceiling", "[", 
       RowBox[{"deg", "/", "2"}], "]"}]}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"lc", "=", 
      RowBox[{"Coefficient", "[", 
       RowBox[{"poly", ",", "x", ",", "deg"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"deg", "<", "2"}], ",", " ", 
       RowBox[{"Return", "[", 
        RowBox[{
         RowBox[{"poly", " ", "//", " ", "Factor"}], ",", " ", "Module"}], 
        "]"}]}], "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"divs", "=", 
      RowBox[{"Most", " ", "@", " ", 
       RowBox[{"Divisors", "[", "deg", "]"}]}]}], ";", "\[IndentingNewLine]", 
     
     RowBox[{"cl", "=", 
      RowBox[{"CoefficientList", "[", 
       RowBox[{"poly", ",", "x"}], "]"}]}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"Do", "[", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"!", 
           RowBox[{"IntegerQ", "[", 
            SuperscriptBox["lc", 
             RowBox[{"d", "/", "deg"}]], "]"}]}], ",", 
          RowBox[{"Continue", "[", "]"}]}], "]"}], ";", "\[IndentingNewLine]", 
        RowBox[{"excess", "=", 
         RowBox[{"Sum", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"C", "[", "n", "]"}], 
            SuperscriptBox["x", "n"]}], ",", 
           RowBox[{"{", 
            RowBox[{"n", ",", "0", ",", "maxdeg"}], "}"}]}], "]"}]}], ";", 
        RowBox[{"(*", " ", 
         RowBox[{
          RowBox[{"Hack", "!!"}], " ", "Setting", " ", "the", " ", "max", " ",
           "to", " ", "deg", " ", "takes", " ", "too", " ", "long", " ", "in",
           " ", 
          RowBox[{"FindInstance", "."}]}], " ", "*)"}], "\[IndentingNewLine]", 
        RowBox[{"factored", "=", 
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{
            RowBox[{"Sum", "[", 
             RowBox[{
              RowBox[{
               RowBox[{"V", "[", "n", "]"}], 
               SuperscriptBox["x", "n"]}], ",", 
              RowBox[{"{", 
               RowBox[{"n", ",", "0", ",", 
                RowBox[{"d", "-", "1"}]}], "}"}]}], "]"}], "+", 
            RowBox[{
             SuperscriptBox["lc", 
              RowBox[{"d", "/", "deg"}]], " ", 
             SuperscriptBox["x", "d"]}]}], ")"}], 
          RowBox[{"deg", "/", "d"}]]}], ";", 
        RowBox[{"(*", " ", 
         RowBox[{"Hack", "!!"}], " ", "*)"}], "\[IndentingNewLine]", 
        RowBox[{"expandedForm", "=", 
         RowBox[{"Expand", "[", 
          RowBox[{"excess", "+", "factored"}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"eqns", "=", 
         RowBox[{"Thread", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"PadLeft", "[", 
             RowBox[{
              RowBox[{"CoefficientList", "[", 
               RowBox[{"expandedForm", ",", "x"}], "]"}], ",", " ", 
              RowBox[{"Length", "[", "cl", "]"}]}], "]"}], "-", "cl"}], "==", 
           "0"}], "]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"unks", "=", 
         RowBox[{"Union", " ", "@", " ", 
          RowBox[{"Cases", "[", 
           RowBox[{"eqns", ",", 
            RowBox[{
             RowBox[{"C", "[", "_", "]"}], "|", 
             RowBox[{"V", "[", "_", "]"}]}], ",", 
            RowBox[{"{", 
             RowBox[{"0", ",", "Infinity"}], "}"}]}], "]"}]}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"soln", "=", 
         RowBox[{"TimeConstrained", "[", 
          RowBox[{
           RowBox[{"FindInstance", "[", 
            RowBox[{"eqns", ",", "unks", ",", "Integers"}], "]"}], ",", 
           "maxtime", ",", 
           RowBox[{"{", "}"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"soln", "=!=", 
            RowBox[{"{", "}"}]}], " ", "&&", " ", 
           RowBox[{"MatchQ", "[", 
            RowBox[{
             RowBox[{"factored", "/.", 
              RowBox[{"soln", "[", 
               RowBox[{"[", "1", "]"}], "]"}]}], ",", " ", 
             RowBox[{"Power", "[", 
              RowBox[{"_Plus", ",", "_"}], "]"}]}], "]"}]}], ",", " ", 
          "\[IndentingNewLine]", 
          RowBox[{"Return", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"partialFactor", "[", 
              RowBox[{
               RowBox[{"excess", "/.", 
                RowBox[{"soln", "[", 
                 RowBox[{"[", "1", "]"}], "]"}]}], ",", "x"}], "]"}], "+", 
             RowBox[{"(", 
              RowBox[{"factored", "/.", 
               RowBox[{"soln", "[", 
                RowBox[{"[", "1", "]"}], "]"}]}], ")"}]}], ",", " ", 
            "Module"}], "]"}]}], "\[IndentingNewLine]", "]"}]}], ",", 
